<div class="contents row">
  <% @tweets.each do |tweet| %>
    <div class="content_post">
      <div class="more">
        <span><%= image_tag 'arrow_top.png' %></span>
      </div>
      <div>
        <a href="/">
          <span>投稿者:</span><%= tweet.name %>
        </a>
      </div>
        <div class="tweet_text"><%= tweet.text %></div>
    </div>
  <% end %>
</div>